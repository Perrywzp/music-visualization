<% include header %>
  <header>
    <h1><%= title %></h1>
    <ul class="type" id="type">
      <li data-type="dot">Dot</li>
      <li data-type="column" class="selected">Column</li>
    </ul>
    <div class="lay">
      <span>Volume</span> <input id="volume" type="range" min="0" max="100" value="60">
    </div>
  </header>
  <div class="left">
    <ul id="list">
      <% music.forEach(function(name) { %>
      <li title="<%= name %>"><%= name %></li>
      <% }) %>
    </ul>
  </div>
  <div class="right" id="box"></div>

<% include footer %>
